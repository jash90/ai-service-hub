# AI Service Hub Configuration
# Copy this file to .env and configure with your API keys and settings

# Environment Configuration
NODE_ENV=development
LOG_LEVEL=info
PORT=3000

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_TIMEOUT=30000
OPENAI_RETRY_ATTEMPTS=3

# DeepSeek Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DEEPSEEK_TIMEOUT=30000
DEEPSEEK_RETRY_ATTEMPTS=3

# Gemini Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_TIMEOUT=30000
GEMINI_RETRY_ATTEMPTS=3

# Claude Configuration
CLAUDE_API_KEY=your_claude_api_key_here
CLAUDE_BASE_URL=https://api.anthropic.com
CLAUDE_TIMEOUT=30000
CLAUDE_RETRY_ATTEMPTS=3

# Grok Configuration
GROK_API_KEY=your_grok_api_key_here
GROK_BASE_URL=https://api.x.ai/v1
GROK_TIMEOUT=30000
GROK_RETRY_ATTEMPTS=3

# Perplexity Configuration
PERPLEXITY_API_KEY=your_perplexity_api_key_here
PERPLEXITY_BASE_URL=https://api.perplexity.ai
PERPLEXITY_TIMEOUT=30000
PERPLEXITY_RETRY_ATTEMPTS=3

# Ollama Configuration (local LLM server)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_TIMEOUT=60000
OLLAMA_RETRY_ATTEMPTS=3

# LM Studio Configuration (local LLM server)
LMSTUDIO_BASE_URL=http://localhost:1234
LMSTUDIO_TIMEOUT=60000
LMSTUDIO_RETRY_ATTEMPTS=3

# Qdrant Configuration (vector database)
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_TIMEOUT=30000
QDRANT_RETRY_ATTEMPTS=3

# Performance Configuration
ENABLE_METRICS=true
SLOW_REQUEST_THRESHOLD=5000
CACHE_ENABLED=false
CACHE_TTL=3600

# Rate Limiting
RATE_LIMIT_ENABLED=false
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Monitoring
MONITORING_ENABLED=true
METRICS_PORT=9090

