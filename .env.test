# Test Environment Configuration
NODE_ENV=test
LOG_LEVEL=error
PORT=3001

# Test API Keys (use your actual keys if you want to run integration tests)
# OPENAI_API_KEY=your_test_openai_key
# DEEPSEEK_API_KEY=your_test_deepseek_key
# GEMINI_API_KEY=your_test_gemini_key
# CLAUDE_API_KEY=your_test_claude_key

# Performance settings for testing
ENABLE_METRICS=true
SLOW_REQUEST_THRESHOLD=10000
CACHE_ENABLED=false

# Monitoring disabled in tests
MONITORING_ENABLED=false
RATE_LIMIT_ENABLED=false

# Test logging
SILENT_TESTS=true